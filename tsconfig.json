{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "astro/client"
    ],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": true
  }
}