{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strict": true,
    "types": ["astro/client", "vite-plugin-pwa/client", "@astrojs/image/client"],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": false
  }
}
