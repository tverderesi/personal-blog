{
  "plugins": ["require.resolve('prettier-plugin-astro')"],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": true,
  "overrides": [
    {
      "files": [".*", "*.json", "*.md", "*.toml", "*.yml", "*.astro"],
      "options": {
        "useTabs": false,
        "parser": "astro"
      }
    }
  ]
}
